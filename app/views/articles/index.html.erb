<h1>Articles</h1>

<table class="zebra-striped">
  <thead>
    <tr>
      <th>Title</th>
      <th>Source</th>
    </tr>
  </thead>
  <tbody>
<% @articles.each do |article| %>
    <tr>
      <td><%= link_to article.title, article %></td>
      <td><%= article.source %></td>
    </tr>
<% end %>
  </tbody>
</table>
